{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "0.1.0",
    "description": "My First Extension",
    "permissions": ["activeTab", "scripting", "tabs"],
    "action": {
        "default_title": "My Extension",
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
